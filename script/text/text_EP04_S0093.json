{
    "scenario_text": [
        {
            "key": "EP04_S0093_0001",
            "value": "おい、ミウ～！\n大丈夫かーっ！！"
        },
        {
            "key": "EP04_S0093_0002",
            "value": "いるなら返事をしろーっ！"
        },
        {
            "key": "EP04_S0093_0003",
            "value": "ミウちゃん、どこにもいないわね……"
        },
        {
            "key": "EP04_S0093_0004",
            "value": "う、うっ……\nまた何か来たわ……"
        },
        {
            "key": "EP04_S0093_0005",
            "value": "お、おいそこのお前！"
        },
        {
            "key": "EP04_S0093_0006",
            "value": "ひ、ひぃ～！"
        },
        {
            "key": "EP04_S0093_0007",
            "value": "怖がらんでも大丈夫じゃ、ピヨモン\nワシもいるんじゃからの"
        },
        {
            "key": "EP04_S0093_0008",
            "value": "ふ……ふえええ……！\nびっくりしたよ～！"
        },
        {
            "key": "EP04_S0093_0009",
            "value": "ミウは！？\nお前たちの女王様はどこに行った！？"
        },
        {
            "key": "EP04_S0093_0010",
            "value": "女王様は、じょ、\n城内のどこかにお隠れです～"
        },
        {
            "key": "EP04_S0093_0011",
            "value": "それって、逃げたってこと？"
        },
        {
            "key": "EP04_S0093_0012",
            "value": "は、はい～！\nそうなんです～！"
        },
        {
            "key": "EP04_S0093_0013",
            "value": "なるほど……あいつらしい。\n逃げ足だけはさすがだな"
        },
        {
            "key": "EP04_S0093_0014",
            "value": "良かった……\n無事なら良いけど……"
        },
        {
            "key": "EP04_S0093_0015",
            "value": "咄嗟の判断にしちゃ、上出来じゃねえか"
        },
        {
            "key": "EP04_S0093_0016",
            "value": "とはいえ、安心なぞできんぞ"
        },
        {
            "key": "EP04_S0093_0017",
            "value": "そうよね……"
        },
        {
            "key": "EP04_S0093_0018",
            "value": "いつまでも無事とは限らないしな\n早いとこ保護しないと"
        },
        {
            "key": "EP04_S0093_0019",
            "value": "まあ小学生の女の子の足だし\nまだこの近くにはいると思うよ"
        },
        {
            "key": "EP04_S0093_0020",
            "value": "じゃあ、またゆーえんちの中、捜すのか？"
        },
        {
            "key": "EP04_S0093_0021",
            "value": "ああ。敵がいて、大変かもしれないけど\nなんとか手分けして、ミウを見つけ出さないと"
        },
        {
            "key": "EP04_S0093_0022",
            "value": "この状況では、やむを得ないか……"
        },
        {
            "key": "EP04_S0093_0023",
            "value": "もしアイツらに捕まったら\n簡単には逃げ出せねえだろうからな……"
        },
        {
            "key": "EP04_S0093_0024",
            "value": "任せて！　もうすっかり敷地の中は覚えたわ！"
        },
        {
            "key": "EP04_S0093_0025",
            "value": "敵よりも先に、<color=#B41919>ミウ</color>を見つけましょ！"
        },
        {
            "key": "EP04_S0093_0026",
            "value": "ボクも、必ず役に立ってみせるよ……！"
        },
        {
            "key": "EP04_S0093_0027",
            "value": "ハル、ミユキ……２人にもまた\n手伝ってもらうことになるけど……いいかな？"
        },
        {
            "key": "EP04_S0093_0028",
            "value": "うん、いいよ"
        },
        {
            "key": "EP04_S0093_0029",
            "value": "いいよ"
        },
        {
            "key": "EP04_S0093_0030",
            "value": "あぶない時は、ボクとタクマが\n守るからね！"
        },
        {
            "key": "EP04_S0093_0031",
            "value": "アグモン、ありがとう……！"
        },
        {
            "key": "EP04_S0093_0032",
            "value": "それじゃみんな……行こう！"
        }
    ]
}