{
	"scenario_text":[
		{
			"key":"EP10_S1080_0001",
			"value":"タクマ！　あれ、あれ見て！！"
		},
		{
			"key":"EP10_S1080_0002",
			"value":"あれは……！？"
		},
		{
			"key":"EP10_S1080_0003",
			"value":"ミノルだーーー！！"
		},
		{
			"key":"EP10_S1080_0004",
			"value":"ミノル！！\nよかった、無事だったのか！！"
		},
		{
			"key":"EP10_S1080_0005",
			"value":"…………"
		},
		{
			"key":"EP10_S1080_0006",
			"value":"ミノル……！？"
		},
		{
			"key":"EP10_S1080_0007",
			"value":"……タクマ"
		},
		{
			"key":"EP10_S1080_0008",
			"value":"どうしたんだ？\nミノルも酷い幻を見たのか？"
		},
		{
			"key":"EP10_S1080_0009",
			"value":"……自分でも、わからない\nいったい、何があったのか……何が起こってるのか……"
		},
		{
			"key":"EP10_S1080_0010",
			"value":"頭が、ボーッとして……"
		},
		{
			"key":"EP10_S1080_0011",
			"value":"タクマ……ここ、かなりやばいところだぞ\n一旦撤退した方がいい"
		},
		{
			"key":"EP10_S1080_0012",
			"value":"なあ……\nすぐ、ここから……"
		},
		{
			"key":"EP10_S1080_0013",
			"value":"うん、危険なのは僕にもわかる\nでも逃げるなら、まずはみんなを見つけないと"
		},
		{
			"key":"EP10_S1080_0014",
			"value":"だから、逃げるならミノルだけで外に出ててくれ\n僕は、もう少し中を捜す"
		},
		{
			"key":"EP10_S1080_0015",
			"value":"いや……それは嫌だ！　一緒にいてくれよ！"
		},
		{
			"key":"EP10_S1080_0016",
			"value":"頼むよ、もう逃げるなんて言わないから！\nオレも一緒にいさせてくれ！"
		},
		{
			"key":"EP10_S1080_0017",
			"value":"お、落ち着いてよ～\n大丈夫、置いていったりしないよ"
		},
		{
			"key":"EP10_S1080_0018",
			"value":"あ？　あ……ああ"
		},
		{
			"key":"EP10_S1080_0019",
			"value":"（ミノル……？）"
		},
		{
			"key":"EP10_S1080_0020",
			"value":"（違和感がある\n　うまく言葉にできないけど、何か引っかかる……）"
		},
		{
			"key":"EP10_S1080_0021",
			"value":"そういえば、ファルコモンは？"
		},
		{
			"key":"EP10_S1080_0022",
			"value":"えっ……？\nふぁる……？"
		},
		{
			"key":"EP10_S1080_0023",
			"value":"ファルコモンだよ\n一緒にいないのか？"
		},
		{
			"key":"EP10_S1080_0024",
			"value":"いや、それは……まあ……"
		},
		{
			"key":"EP10_S1080_0025",
			"value":"見失ったのか？"
		},
		{
			"key":"EP10_S1080_0026",
			"value":"あ、ああ、そうだ\nあいつふらっとどこかに行っちゃって"
		},
		{
			"key":"EP10_S1080_0027",
			"value":"ファルコモンが？\nミノルを放って、ふらっと？"
		},
		{
			"key":"EP10_S1080_0028",
			"value":"（そんなの絶対に変だ\n　あんなにぶつかり合って絆を深めた二人が……）"
		},
		{
			"key":"EP10_S1080_0029",
			"value":"（こんな風にあっさりとお互い\n　目の届かないところに行くなんて）"
		},
		{
			"key":"EP10_S1080_0030",
			"value":"なあ、そんなことより\n早くみんなを捜して、早く出ようぜ"
		},
		{
			"key":"EP10_S1080_0031",
			"value":"そんなことより……って\nファルコモンのことを、そんなことって言ったの？"
		},
		{
			"key":"EP10_S1080_0032",
			"value":"…………"
		},
		{
			"key":"EP10_S1080_0033",
			"value":"お前……誰だ？"
		},
		{
			"key":"EP10_S1080_0034",
			"value":"えっ？"
		},
		{
			"key":"EP10_S1080_0035",
			"value":"昔ならともかく、今のミノルは絶対に\nファルコモンをないがしろになんかしない"
		},
		{
			"key":"EP10_S1080_0036",
			"value":"お前はシュウジさんたちと同じ幻か？\nそれとも……！"
		},
		{
			"key":"EP10_S1080_0037",
			"value":"……うう"
		},
		{
			"key":"EP10_S1080_0038",
			"value":"うわあぁぁぁぁぁぁぁ！"
		},
		{
			"key":"EP10_S1080_0039",
			"value":"！！！"
		},
		{
			"key":"EP10_S1080_0040",
			"value":"なんで避けるんだよ、タクマぁっ！\n避けなけりゃ楽にしてやれたのによぉ"
		},
		{
			"key":"EP10_S1080_0041",
			"value":"お前、ミノルじゃないな\n正体を見せろ！！"
		},
		{
			"key":"EP10_S1080_0042",
			"value":"ウ、ウウ……オオオオオオオオ！！"
		},
		{
			"key":"EP10_S1080_0043",
			"value":"タクマ、下がって！\n偽ミノルめ、ボクが相手だ！"
		}
	]
}
