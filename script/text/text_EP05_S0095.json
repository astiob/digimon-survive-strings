{
	"scenario_text":[
		{
			"key":"EP05_S0095_0001",
			"value":"シュウジさん、どこに行ったんだ……"
		},
		{
			"key":"EP05_S0095_0002",
			"value":"おーい！　タクマー！"
		},
		{
			"key":"EP05_S0095_0003",
			"value":"ミノル！　みんなも！"
		},
		{
			"key":"EP05_S0095_0004",
			"value":"シュウジさん、こっちにはいなかった"
		},
		{
			"key":"EP05_S0095_0005",
			"value":"こっちもだ"
		},
		{
			"key":"EP05_S0095_0006",
			"value":"みんな見つけられなかったみたい\nあとはこの辺りだけだけど……"
		},
		{
			"key":"EP05_S0095_0007",
			"value":"あれ？　オレのフィギュアが消えてく……"
		},
		{
			"key":"EP05_S0095_0008",
			"value":"あ……私のCDが、消えていく……"
		},
		{
			"key":"EP05_S0095_0009",
			"value":"あ、私の、カルテ……消えてく……"
		},
		{
			"key":"EP05_S0095_0010",
			"value":"ほっ……"
		},
		{
			"key":"EP05_S0095_0011",
			"value":"あ……ウチのお守り、消えちゃう……"
		},
		{
			"key":"EP05_S0095_0012",
			"value":"ママの金メダル……消えてく……"
		},
		{
			"key":"EP05_S0095_0013",
			"value":"なんだよ、ただの幻ってこと？\nタチが悪いなー"
		},
		{
			"key":"EP05_S0095_0014",
			"value":"（幻……そうか、だからカメラでしか\n　見れなかったのか）"
		},
		{
			"key":"EP05_S0095_0015",
			"value":"（みんなの反応からして、それぞれの事情とか\n　人生にまで関わるほど大事なものだったんだろうな）"
		},
		{
			"key":"EP05_S0095_0016",
			"value":"あ、あそこ！　シュウジとロップモンだ！"
		},
		{
			"key":"EP05_S0095_0017",
			"value":"ホントだ！　おーい！　シュウジさーん！"
		},
		{
			"key":"EP05_S0095_0018",
			"value":"シュウジさーん！"
		},
		{
			"key":"EP05_S0095_0019",
			"value":"おい！　聞こえねえのか！？　シュウジ！！"
		},
		{
			"key":"EP05_S0095_0020",
			"value":"ねぇ、シュウジ……"
		},
		{
			"key":"EP05_S0095_0021",
			"value":"…………"
		},
		{
			"key":"EP05_S0095_0022",
			"value":"シュウジぃ……シュウジってばぁ……"
		},
		{
			"key":"EP05_S0095_0023",
			"value":"聞こえてないのか？　そんな離れてないのに"
		},
		{
			"key":"EP05_S0095_0024",
			"value":"ね、ねぇ……なんか、シュウジさん\nふらふら歩いてない？"
		},
		{
			"key":"EP05_S0095_0025",
			"value":"奥に向かってるのか？"
		},
		{
			"key":"EP05_S0095_0026",
			"value":"おい、明らかに様子がおかしいだろ"
		},
		{
			"key":"EP05_S0095_0027",
			"value":"そうだね\nみんな、シュウジさんの後を追おう！"
		}
	]
}
