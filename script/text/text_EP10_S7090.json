{
	"scenario_text":[
		{
			"key":"EP10_S7090_0001",
			"value":"……ん？　あれは！！"
		},
		{
			"key":"EP10_S7090_0002",
			"value":"ミウ！\nこんなところにいたのか！"
		},
		{
			"key":"EP10_S7090_0003",
			"value":"タクマさん……！　やっと来てくれたぁ！\nウチ、さっきから１人で怖くて……！"
		},
		{
			"key":"EP10_S7090_0004",
			"value":"ご、ごめん"
		},
		{
			"key":"EP10_S7090_0005",
			"value":"タクマさん……\nごめんなさい、ウチ、腰が抜けてて立てない……"
		},
		{
			"key":"EP10_S7090_0006",
			"value":"大丈夫かー？"
		},
		{
			"key":"EP10_S7090_0007",
			"value":"あまり無理しないで、少し休もう"
		},
		{
			"key":"EP10_S7090_0008",
			"value":"ううん、こんなところで道草は食ってらんないの……\n早くアオイさんを捜さなきゃ……！"
		},
		{
			"key":"EP10_S7090_0009",
			"value":"ウチ、もう誰にもひどい目にあってほしくない……！"
		},
		{
			"key":"EP10_S7090_0010",
			"value":"う……ん"
		},
		{
			"key":"EP10_S7090_0011",
			"value":"タクマ……？"
		},
		{
			"key":"EP10_S7090_0012",
			"value":"（何か、おかしいな……）"
		},
		{
			"key":"EP10_S7090_0013",
			"value":"（違和感がある\n　うまく言葉にできないけど、何か引っかかる……）"
		},
		{
			"key":"EP10_S7090_0014",
			"value":"（違和感は、ミウに話しかけた時から……？\n　いや、ミウを見かけた時から……）"
		},
		{
			"key":"EP10_S7090_0015",
			"value":"（そもそも、ミウはいつからここにいたんだ？）"
		},
		{
			"key":"EP10_S7090_0016",
			"value":"（それに、シャコモンはどこに……？）"
		},
		{
			"key":"EP10_S7090_Q1",
			"value":"（僕が感じる違和感は……）"
		},
		{
			"key":"EP10_S7090_Q1_A1",
			"value":"ミウはいつからいたんだ？"
		},
		{
			"key":"EP10_S7090_Q1_A2",
			"value":"シャコモンはどこにいったんだ？"
		},
		{
			"key":"EP10_S7090_0017",
			"value":"なあ……ミウはいつからそこにいたんだ？"
		},
		{
			"key":"EP10_S7090_0018",
			"value":"シュウジさんとリョウに出くわした直後には\nここにミウはいなかった"
		},
		{
			"key":"EP10_S7090_0019",
			"value":"…………"
		},
		{
			"key":"EP10_S7090_0020",
			"value":"ミウは最初からここにいたのか？"
		},
		{
			"key":"EP10_S7090_0021",
			"value":"………………"
		},
		{
			"key":"EP10_S7090_0022",
			"value":"ミウ？"
		},
		{
			"key":"EP10_S7090_0023",
			"value":"シャコモンは、どこにいったんだ？"
		},
		{
			"key":"EP10_S7090_0024",
			"value":"シャコモン……？\nさあ……ウチ、わかんないよ"
		},
		{
			"key":"EP10_S7090_0025",
			"value":"わかんないって……\nずっと一緒にいたはずだろ？"
		},
		{
			"key":"EP10_S7090_0026",
			"value":"タクマさんは、ウチよりも\nシャコモンが心配なの！？"
		},
		{
			"key":"EP10_S7090_0027",
			"value":"そ、そうじゃないけど……"
		},
		{
			"key":"EP10_S7090_0028",
			"value":"ねえ……いいから早くおんぶしてよ\nウチ、立ち上がれないよ"
		},
		{
			"key":"EP10_S7090_0029",
			"value":"あ、ああ……わかったよ\nほら、僕の背中に……"
		},
		{
			"key":"EP10_S7090_0030",
			"value":"（何だ……背筋がぞくぞくする……\n　おかしい……！）"
		},
		{
			"key":"EP10_S7090_0031",
			"value":"タクマ……ミウ、なんか変だよ！"
		},
		{
			"key":"EP10_S7090_0032",
			"value":"なんで……なんでででででで\nそんなこと、言うのおおおおお！？"
		},
		{
			"key":"EP10_S7090_0033",
			"value":"お前……ミウじゃないな！？\n正体を現せ！！"
		},
		{
			"key":"EP10_S7090_0034",
			"value":"ウ、ウウ……オオオオオオオオ！！"
		},
		{
			"key":"EP10_S7090_0035",
			"value":"タクマ！　敵だよ！！"
		}
	]
}
