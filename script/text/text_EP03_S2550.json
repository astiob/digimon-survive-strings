{
	"scenario_text":[
		{
			"key":"EP03_S2550_0001",
			"value":"ったく、早くミウを見つけなきゃなんねーのに\nなんでオレがガキの面倒なんて……"
		},
		{
			"key":"EP03_S2550_0002",
			"value":"へへ、いいじゃねえか\n寂しい一人旅とはオサラバだぜ？"
		},
		{
			"key":"EP03_S2550_0003",
			"value":"寂しいなんて一度も思ったことねーよ"
		},
		{
			"key":"EP03_S2550_0004",
			"value":"……あ"
		},
		{
			"key":"EP03_S2550_0005",
			"value":"どうしたの？　お姉ちゃん"
		},
		{
			"key":"EP03_S2550_0006",
			"value":"あ？　おい、どうした\n付いてこねえなら置いてくぞ"
		},
		{
			"key":"EP03_S2550_0007",
			"value":"あぁん？　何をぼーっと見てんだ？　お嬢ちゃん"
		},
		{
			"key":"EP03_S2550_0008",
			"value":"何を見てんだ？"
		},
		{
			"key":"EP03_S2550_0009",
			"value":"あれは……ゴンドラ？"
		},
		{
			"key":"EP03_S2550_0010",
			"value":"途中で止まってるな"
		},
		{
			"key":"EP03_S2550_0011",
			"value":"みんながあそこで捕まっちゃったみたい"
		},
		{
			"key":"EP03_S2550_0012",
			"value":"おいおい、それってつまり、あの女の仕業か！？"
		},
		{
			"key":"EP03_S2550_0013",
			"value":"チッ！　やっぱりバケモノだったか！\nだからあれ程言ってやったのに……！"
		},
		{
			"key":"EP03_S2550_0014",
			"value":"みんな、どうなっちゃうんだろう？"
		},
		{
			"key":"EP03_S2550_0015",
			"value":"く……"
		},
		{
			"key":"EP03_S2550_0016",
			"value":"…………っ！"
		},
		{
			"key":"EP03_S2550_0017",
			"value":"あぁくそっ！　助けに行くから\nお前らは隠れて待ってるんだ！"
		},
		{
			"key":"EP03_S2550_0018",
			"value":"行くぞ、ドラクモン！"
		},
		{
			"key":"EP03_S2550_0019",
			"value":"あいよ！"
		}
	]
}
