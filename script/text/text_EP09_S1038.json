{
	"scenario_text":[
		{
			"key":"EP09_S1038_0001",
			"value":"あ！　あなた、ヒドイ怪我！"
		},
		{
			"key":"EP09_S1038_0002",
			"value":"ミウ待って！　あんまり動かしちゃダメ！"
		},
		{
			"key":"EP09_S1038_0003",
			"value":"あ、あぁ……ミウ、さま……"
		},
		{
			"key":"EP09_S1038_0004",
			"value":"待ってて、すぐ助けてあげるからね！"
		},
		{
			"key":"EP09_S1038_0005",
			"value":"こんな怪我で、助かるのか……？"
		},
		{
			"key":"EP09_S1038_0006",
			"value":"……たぶん、もうダメだと、思う"
		},
		{
			"key":"EP09_S1038_0007",
			"value":"ミウさま……来てくれて、ありがとう……\n早く、逃げて……"
		},
		{
			"key":"EP09_S1038_0008",
			"value":"何言ってんの！　あなたも一緒に……"
		},
		{
			"key":"EP09_S1038_0009",
			"value":"ミウ……この子の最期の言葉、聞いてあげて"
		},
		{
			"key":"EP09_S1038_0010",
			"value":"さ、最期って……"
		},
		{
			"key":"EP09_S1038_0011",
			"value":"あぁ……また、ミウさまに会えて\nよかった、な……"
		},
		{
			"key":"EP09_S1038_0012",
			"value":"う、うそ……"
		},
		{
			"key":"EP09_S1038_0013",
			"value":"間に合わなかった……"
		},
		{
			"key":"EP09_S1038_0014",
			"value":"そんなぁ……うぐっ、ぐすっ……"
		},
		{
			"key":"EP09_S1038_0015",
			"value":"ミウ……"
		},
		{
			"key":"EP09_S1038_0016",
			"value":"ぐすっ……大丈夫\n悲しいけど……すっごく悲しいけど、泣くのは後"
		},
		{
			"key":"EP09_S1038_0017",
			"value":"今は、少しでも多く\nみんなを助けないと！"
		},
		{
			"key":"EP09_S1038_0018",
			"value":"……ああ、そうだね"
		},
		{
			"key":"EP09_S1038_0019",
			"value":"ミウ、偉いわ\n強くなったわね"
		},
		{
			"key":"EP09_S1038_0020",
			"value":"行こう、シャコモン、みんな！"
		}
	]
}
