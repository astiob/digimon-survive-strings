{
    "scenario_text": [
        {
            "key": "EP02_S1700_0001",
            "value": "教授が無事でよかったよな"
        },
        {
            "key": "EP02_S1700_0002",
            "value": "そうだな\nあれを無事というかは別として"
        },
        {
            "key": "EP02_S1700_0003",
            "value": "サキとシュウジさんがいるのもわかったし\nあとはちゃんと見つけてやるだけだな！"
        },
        {
            "key": "EP02_S1700_0004",
            "value": "うむ！　成し遂げてみせるさ！"
        },
        {
            "key": "EP02_S1700_0005",
            "value": "あれ？　なぁタクマ\nあそこにあるのって足跡じゃね？"
        },
        {
            "key": "EP02_S1700_0006",
            "value": "え？　どれだ？"
        },
        {
            "key": "EP02_S1700_0007",
            "value": "ほら、そこの草むらのとこ"
        },
        {
            "key": "EP02_S1700_Q1",
            "value": "（ミノルが言う足跡なんて見えないぞ？）"
        },
        {
            "key": "EP02_S1700_Q1_A1",
            "value": "どうやって見つけたんだ？"
        },
        {
            "key": "EP02_S1700_Q1_A2",
            "value": "本当にあるのか？"
        },
        {
            "key": "EP02_S1700_0008",
            "value": "僕には見えないけど、どうやって見つけたんだ？"
        },
        {
            "key": "EP02_S1700_0009",
            "value": "ボク見えるかも～"
        },
        {
            "key": "EP02_S1700_0010",
            "value": "俺もだ。見えるというより、感じる、だが"
        },
        {
            "key": "EP02_S1700_0011",
            "value": "あぁごめん、スマホのカメラで見てたんだ"
        },
        {
            "key": "EP02_S1700_0012",
            "value": "ほら、これ"
        },
        {
            "key": "EP02_S1700_0013",
            "value": "本当にあるのか？"
        },
        {
            "key": "EP02_S1700_0014",
            "value": "今ウソつくよーなタイミングじゃねーだろ！\nホントにあるんだって！"
        },
        {
            "key": "EP02_S1700_0015",
            "value": "でも全然見えないぞ？"
        },
        {
            "key": "EP02_S1700_0016",
            "value": "またイタズラか～？"
        },
        {
            "key": "EP02_S1700_0017",
            "value": "こらこら、いわれのない非難はよせ"
        },
        {
            "key": "EP02_S1700_0018",
            "value": "カメラで見ればわかりやすいって！\nほら"
        },
        {
            "key": "EP02_S1700_0019",
            "value": "こっからじゃわかりにくいかな？\nアップしてみれば……"
        },
        {
            "key": "EP02_S1700_0020",
            "value": "（ミノルのスマホの画面には\n　確かに足跡のようなものが見えるな）"
        },
        {
            "key": "EP02_S1700_0021",
            "value": "（僕も<color=#B41919>カメラ</color>で見てみよう）"
        },
        {
            "key": "EP02_S1700_0022",
            "value": "な？　足跡みたいだろ？"
        },
        {
            "key": "EP02_S1700_0023",
            "value": "（僕もカメラで見てみよう）"
        }
    ]
}