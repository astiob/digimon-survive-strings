{
	"scenario_text":[
		{
			"key":"EP03_S2110_0001",
			"value":"（あれ？\nサキがこんな所で……）"
		},
		{
			"key":"EP03_S2110_0002",
			"value":"あいたたた……\n足をヒネっちゃった……"
		},
		{
			"key":"EP03_S2110_Q1",
			"value":"う～ん、どうしよう？"
		},
		{
			"key":"EP03_S2110_Q1_A1",
			"value":"ぼくが看てあげよう"
		},
		{
			"key":"EP03_S2110_Q1_A2",
			"value":"前を見て歩かないからだよ"
		},
		{
			"key":"EP03_S2110_Q1_A3",
			"value":"見て見ぬフリしよっと……"
		},
		{
			"key":"EP03_S2110_0003",
			"value":"どれ、ぼくが看てあげよう\n捻挫だったら大変だし"
		},
		{
			"key":"EP03_S2110_0004",
			"value":"……どう？　痛い？"
		},
		{
			"key":"EP03_S2110_0005",
			"value":"んんっ……わかんない"
		},
		{
			"key":"EP03_S2110_0006",
			"value":"歩ける？"
		},
		{
			"key":"EP03_S2110_0007",
			"value":"う、うん……イテテテ！"
		},
		{
			"key":"EP03_S2110_0008",
			"value":"筋を痛めただけのようだけど、\nあとしばらく歩くのも辛いだろうな"
		},
		{
			"key":"EP03_S2110_0009",
			"value":"じゃあ、ぼくがおぶってあげるよ"
		},
		{
			"key":"EP03_S2110_0010",
			"value":"えっ！？　ヤだ、\nいい、いいって恥ずかしい！"
		},
		{
			"key":"EP03_S2110_0011",
			"value":"……でも、ありがとう"
		},
		{
			"key":"EP03_S2110_0012",
			"value":"ちゃんと前を見て歩かないからだよ\nいつもいつもフラフラして……"
		},
		{
			"key":"EP03_S2110_0013",
			"value":"（サキにも、たまにはビシっと\n厳しい所見せなきゃな……）"
		},
		{
			"key":"EP03_S2110_0014",
			"value":"うぅ～！\nひとが痛くて苦しんでる時に～！"
		},
		{
			"key":"EP03_S2110_0015",
			"value":"そんなお説教は何の意味もないでしょ～！\nタクマのいじわる～！"
		},
		{
			"key":"EP03_S2110_0016",
			"value":"見て見ぬフリしよっと……\nは、ぼくは知らない……"
		},
		{
			"key":"EP03_S2110_0017",
			"value":"あーも～！　タクマったら！\n私のこと見捨てて行くんだ～！"
		},
		{
			"key":"EP03_S2110_0018",
			"value":"あ、ぼくに気づいてたのか……"
		},
		{
			"key":"EP03_S2110_0019",
			"value":"少し気まずい……"
		}
	]
}
