	"scenario_text":[
		{
			"key":"EP10_S9072_0001",
			"value":"教授、今です！"
		},
		{
			"key":"EP10_S9072_0002",
			"value":"タオモン、聞いてくれ！\nお前は主に踊らされているだけだ"
		},
		{
			"key":"EP10_S9072_0003",
			"value":"四聖獣を倒せば主を縛る結界が崩壊し\n全てが終わるんだ！"
		},
		{
			"key":"EP10_S9072_0004",
			"value":"お前の言葉に何の意味がある\n裏切り者め"
		},
		{
			"key":"EP10_S9072_0005",
			"value":"今度こそ終わりにしてやる！"
		},
		{
			"key":"EP10_S9072_0006",
			"value":"くっ……ダメか……！"
		},
		{
			"key":"EP10_S9072_0007",
			"value":"諦めないでください、教授！"
		},
		{
			"key":"EP10_S9072_0008",
			"value":"タクマくん……うむ、そうだな\nこの命がある限り、諦めるわけにはいかん……！"
		}
	]
}
