{
	"scenario_text":[
		{
			"key":"EP09_S4020_0001",
			"value":"あれ、ドラクモンひとり？"
		},
		{
			"key":"EP09_S4020_0002",
			"value":"少しタクマに聞いておきたいことがあってな"
		},
		{
			"key":"EP09_S4020_0003",
			"value":"どうしてこっちの世界に戻ってきた？"
		},
		{
			"key":"EP09_S4020_0004",
			"value":"ん～？　どういうこと？"
		},
		{
			"key":"EP09_S4020_Q1",
			"value":"（僕が戻ってきた理由は……）"
		},
		{
			"key":"EP09_S4020_Q1_A1",
			"value":"みんなを守りたいからだ"
		},
		{
			"key":"EP09_S4020_Q1_A2",
			"value":"見て見ぬふりはできないからだ"
		},
		{
			"key":"EP09_S4020_Q1_A3",
			"value":"力があるからだ"
		},
		{
			"key":"EP09_S4020_0005",
			"value":"みんなを守りたい"
		},
		{
			"key":"EP09_S4020_0006",
			"value":"みんなを守りたいからね\n仲間たちも、この世界も、僕らの世界も"
		},
		{
			"key":"EP09_S4020_0007",
			"value":"……なるほどな"
		},
		{
			"key":"EP09_S4020_0008",
			"value":"見て見ぬふりはできない"
		},
		{
			"key":"EP09_S4020_0009",
			"value":"みんなの前でも言ったけど\n見て見ぬふりはできないからね"
		},
		{
			"key":"EP09_S4020_0010",
			"value":"……なるほどな"
		},
		{
			"key":"EP09_S4020_0011",
			"value":"力があるから"
		},
		{
			"key":"EP09_S4020_0012",
			"value":"僕たちには力があるんだ\nだから、その力で解決できると思ってる"
		},
		{
			"key":"EP09_S4020_0013",
			"value":"……なるほどな"
		},
		{
			"key":"EP09_S4020_0014",
			"value":"え？　それだけ？"
		},
		{
			"key":"EP09_S4020_0015",
			"value":"僕、変なこと言ったかな？"
		},
		{
			"key":"EP09_S4020_0016",
			"value":"いや、やっぱカイトとは違うなって\nそう思っただけさ"
		},
		{
			"key":"EP09_S4020_0017",
			"value":"そりゃそうだよ\nカイトと僕じゃ、性格が全然違うんだし"
		},
		{
			"key":"EP09_S4020_0018",
			"value":"だよな\nま、気にしなくていい"
		},
		{
			"key":"EP09_S4020_0019",
			"value":"（……なんだったんだろう）"
		},
		{
			"key":"EP09_S4020_0020",
			"value":"タクマはタクマの心を大事にしろよ\nアグモンのこともな"
		}
	]
}
