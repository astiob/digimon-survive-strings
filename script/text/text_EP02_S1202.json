	"scenario_text":[
		{
			"key":"EP02_S1202_0001",
			"value":"タクマー、ショクリョウはここにあるのか？"
		},
		{
			"key":"EP02_S1202_0002",
			"value":"そう思ってたんだけど……あてが外れたな"
		},
		{
			"key":"EP02_S1202_0003",
			"value":"まー気にするな！\nタクマならなんとかできるさ！"
		},
		{
			"key":"EP02_S1202_0004",
			"value":"はは、ありがとう。頑張ってみるよ"
		},
		{
			"key":"EP02_S1202_0005",
			"value":"ん～？\nなぁタクマー、これってショクリョウか？"
		},
		{
			"key":"EP02_S1202_0006",
			"value":"え？　どれ？"
		},
		{
			"key":"EP02_S1202_0007",
			"value":"あー……食べ物ではないけど\n疲れた時には効くんじゃないかな"
		},
		{
			"key":"EP02_S1202_0008",
			"value":"そうか！　じゃあタクマにあげる！"
		},
		{
			"key":"EP02_S1202_0009",
			"value":"ありがとう、アグモン"
		},
		{
			"key":"EP02_S1202_0010",
			"value":"まだここを探すのかー？"
		}
	]
}
