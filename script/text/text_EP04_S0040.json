{
    "scenario_text": [
        {
            "key": "EP04_S0040_0001",
            "value": "おお～、なんか広いとこに出たぞー"
        },
        {
            "key": "EP04_S0040_0002",
            "value": "中央広場……とか、そういう感じかな"
        },
        {
            "key": "EP04_S0040_0003",
            "value": "ってことは、ここが敷地のど真ん中ってとこか"
        },
        {
            "key": "EP04_S0040_0004",
            "value": "位置関係がわかりづらいな\n地図でもあればよかったんだが……"
        },
        {
            "key": "EP04_S0040_0005",
            "value": "元の世界の遊園地だとさ\nこういうとこでショーとかやったりすんだよな"
        },
        {
            "key": "EP04_S0040_0006",
            "value": "ミノル、ショーとはなんだ？"
        },
        {
            "key": "EP04_S0040_0007",
            "value": "色んなキャラクターが、歌ったり踊ったり\nパレードしたりするんだよ"
        },
        {
            "key": "EP04_S0040_0008",
            "value": "着ぐるみを着たキャラクターが\nわーっと出てたりとかね！"
        },
        {
            "key": "EP04_S0040_0009",
            "value": "へ～！　なんだか楽しそうね！"
        },
        {
            "key": "EP04_S0040_0010",
            "value": "お客さんを巻き込んで、いきなり物語が\n始まったりすることもあるんだよな～！"
        },
        {
            "key": "EP04_S0040_0011",
            "value": "お前たち、ここからは一歩も通さないぞ！\nなんつってさ……！"
        },
        {
            "key": "EP04_S0040_0012",
            "value": "あはは、ありがちだよね"
        },
        {
            "key": "EP04_S0040_0013",
            "value": "それって、ああいう感じ……？"
        },
        {
            "key": "EP04_S0040_0014",
            "value": "感じ……？"
        },
        {
            "key": "EP04_S0040_0015",
            "value": "ん？　ああいうって……？"
        },
        {
            "key": "EP04_S0040_0016",
            "value": "オマエたち！"
        },
        {
            "key": "EP04_S0040_0017",
            "value": "今度こそ、ここから先へは通さないぞ！"
        },
        {
            "key": "EP04_S0040_0018",
            "value": "覚悟しなさい！"
        },
        {
            "key": "EP04_S0040_0019",
            "value": "チッ……また出やがったか"
        },
        {
            "key": "EP04_S0040_0020",
            "value": "さっきは慌てて逃げてったくせにな"
        },
        {
            "key": "EP04_S0040_0021",
            "value": "あ、あれは逃げたのではない！\nセンリャクテキな戦略だったのだ！　である！"
        },
        {
            "key": "EP04_S0040_0022",
            "value": "戦略的な戦略？"
        },
        {
            "key": "EP04_S0040_0023",
            "value": "その言い方はおかしくはないか？\n表現が重複しているじゃないか"
        },
        {
            "key": "EP04_S0040_0024",
            "value": "しゅ、シュウジさん……\nそんな真面目に……"
        },
        {
            "key": "EP04_S0040_0025",
            "value": "こらー！　動くな！\n妙な動きをすれば、今すぐあれだぞ！"
        },
        {
            "key": "EP04_S0040_0026",
            "value": "ん～？　あれってなにー？"
        },
        {
            "key": "EP04_S0040_0027",
            "value": "だから、あれ……それだ！\nやっちゃうんだから！"
        },
        {
            "key": "EP04_S0040_0028",
            "value": "あ、あのね、私たちは\nあなたたちに危害を加えるつもりはないの"
        },
        {
            "key": "EP04_S0040_0029",
            "value": "そうよ！\nさっき遊園地の入り口でも言ったでしょ！"
        },
        {
            "key": "EP04_S0040_0030",
            "value": "でも、ここまで追いかけてきたじゃない！\nワルモノだからしつこく来たに、違いないもん！"
        },
        {
            "key": "EP04_S0040_0031",
            "value": "えっ！？　違うって！"
        },
        {
            "key": "EP04_S0040_0032",
            "value": "僕たちはただ、人を捜しに……"
        },
        {
            "key": "EP04_S0040_0033",
            "value": "だーーーーーっ！！！\nもう、我慢ならねえ！！！"
        },
        {
            "key": "EP04_S0040_0034",
            "value": "お、おい……カイト？"
        },
        {
            "key": "EP04_S0040_0035",
            "value": "これ以上話しても時間の無駄だ！\n邪魔しようってんなら……"
        },
        {
            "key": "EP04_S0040_0036",
            "value": "力ずくで蹴散らしてやる！"
        },
        {
            "key": "EP04_S0040_0037",
            "value": "ひいっ！？\nこここ、怖くなんてないからな！"
        },
        {
            "key": "EP04_S0040_0038",
            "value": "いや結構怯えてるじゃん"
        },
        {
            "key": "EP04_S0040_0039",
            "value": "ちょ、ちょっとかわいそうだよ……"
        },
        {
            "key": "EP04_S0040_0040",
            "value": "カイト、ここは落ち着いて！"
        },
        {
            "key": "EP04_S0040_0041",
            "value": "うるせえ！\nタクマも邪魔すんな！"
        },
        {
            "key": "EP04_S0040_0042",
            "value": "ちっ、こうなりゃ仕方ねえか\nちっと痛い目見せてやるっきゃねえな"
        },
        {
            "key": "EP04_S0040_0043",
            "value": "えぇ！？　あんな小っちゃい子たち相手に戦う気！？"
        },
        {
            "key": "EP04_S0040_0044",
            "value": "気分良くはねーけどよ……\nカイトを怒らせたアイツらにも責任はあんだろ"
        },
        {
            "key": "EP04_S0040_0045",
            "value": "はわわわ……っ\nこっちに来るなー！"
        },
        {
            "key": "EP04_S0040_0046",
            "value": "ま、負けるつもりはないけどぉ……！"
        },
        {
            "key": "EP04_S0040_0047",
            "value": "散々人をおちょくりやがって\n覚悟はできてんだろうな……？"
        },
        {
            "key": "EP04_S0040_0048",
            "value": "（<color=#B41919>カイト</color>は本気だ……！\n　な、なんとかしてこの場を収めないと……！）"
        }
    ]
}