{
	"scenario_text":[
		{
			"key":"EP05_S0341_0001",
			"value":"う～ん、シュウジさん\nどこに行っちゃったんだろう？"
		},
		{
			"key":"EP05_S0341_0002",
			"value":"こっちの方じゃないのかな……？"
		},
		{
			"key":"EP05_S0341_0003",
			"value":"むむむ……"
		},
		{
			"key":"EP05_S0341_0004",
			"value":"あ、聞こえた！　シュウジの声よ！"
		},
		{
			"key":"EP05_S0341_0005",
			"value":"え、ホント！？"
		},
		{
			"key":"EP05_S0341_0006",
			"value":"ええ、ここよりもっと……後ろの方ね！"
		},
		{
			"key":"EP05_S0341_0007",
			"value":"後ろって……どっち？"
		},
		{
			"key":"EP05_S0341_0008",
			"value":"まぁでも、ここにはいないってことだね"
		},
		{
			"key":"EP05_S0341_0009",
			"value":"声が聞こえたのは確かよ\nもうちょっと探ってみるわ"
		}
	]
}
