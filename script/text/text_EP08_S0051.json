{
    "scenario_text": [
        {
            "key": "EP08_S0051_0001",
            "value": "タクマさん……！\nあれ……！"
        },
        {
            "key": "EP08_S0051_0002",
            "value": "ほんとに、ミノルたち以外の\nニンゲンがいた……！"
        },
        {
            "key": "EP08_S0051_0003",
            "value": "ああ……！\nこれで今度こそ、ハッキリした！"
        },
        {
            "key": "EP08_S0051_0004",
            "value": "ここは、間違いなく元の世界だ！"
        },
        {
            "key": "EP08_S0051_0005",
            "value": "そ、そうですね……\nでも……"
        },
        {
            "key": "EP08_S0051_0006",
            "value": "ん～？　ミユキ、どうかした？"
        },
        {
            "key": "EP08_S0051_0007",
            "value": "あ、ううん……なんでもないの\nちょっと、気になることがあっただけ……っ"
        },
        {
            "key": "EP08_S0051_0008",
            "value": "（気になること……？\n　あんな心配そうな顔するほど気になることって……）"
        },
        {
            "key": "EP08_S0051_0009",
            "value": "ミユキ、大丈夫―？"
        },
        {
            "key": "EP08_S0051_0010",
            "value": "う、うん……"
        },
        {
            "key": "EP08_S0051_0011",
            "value": "（あっ……！　もしかしてアグモンのこと！？\n　アグモンが見つかったら、騒ぎになるから……？）"
        },
        {
            "key": "EP08_S0051_0012",
            "value": "（それとも、ミノルたちやみんなが\n　こっちに戻ってるかどうかってことかな？）"
        },
        {
            "key": "EP08_S0051_0013",
            "value": "（いや、それよりなにより\n　ハルのことが心配なのかも……）"
        },
        {
            "key": "EP08_S0051_Q1",
            "value": "（もしかして、ミユキが気にしてるのは……）"
        },
        {
            "key": "EP08_S0051_Q1_A1",
            "value": "ハルが心配？"
        },
        {
            "key": "EP08_S0051_Q1_A2",
            "value": "アグモンが気になる？"
        },
        {
            "key": "EP08_S0051_Q1_A3",
            "value": "みんなのことを気にしてる？"
        },
        {
            "key": "EP08_S0051_0014",
            "value": "ハルが心配？"
        },
        {
            "key": "EP08_S0051_0015",
            "value": "ハルのこと……かな？\n離れてたら、心配だよね"
        },
        {
            "key": "EP08_S0051_0016",
            "value": "え……と、はい……\nそれはもちろん心配だけど……"
        },
        {
            "key": "EP08_S0051_0017",
            "value": "（あれ……？\n　もしかしてハルのことじゃなかったのかな？）"
        },
        {
            "key": "EP08_S0051_0018",
            "value": "アグモンが気になる？"
        },
        {
            "key": "EP08_S0051_0019",
            "value": "アグモンのことだよね？"
        },
        {
            "key": "EP08_S0051_0020",
            "value": "こっちの世界で、アグモンを連れてると\nどうしても目立っちゃうから……"
        },
        {
            "key": "EP08_S0051_0021",
            "value": "そうなのかー？"
        },
        {
            "key": "EP08_S0051_0022",
            "value": "う、うん……\nアグモンのことも、気になるけど……"
        },
        {
            "key": "EP08_S0051_0023",
            "value": "（あれ……もしかして、気になるのは\n　アグモンのことだけじゃないのか……？）"
        },
        {
            "key": "EP08_S0051_0024",
            "value": "みんなのことを気にしてる？"
        },
        {
            "key": "EP08_S0051_0025",
            "value": "みんなも、こっちの世界に戻れてるんじゃないかって\n気になってる……？"
        },
        {
            "key": "EP08_S0051_0026",
            "value": "え……？"
        },
        {
            "key": "EP08_S0051_0027",
            "value": "僕も、気にはなってるんだ"
        },
        {
            "key": "EP08_S0051_0028",
            "value": "だけど、あの時音楽室にいたのは\n僕たち３人だけだったから"
        },
        {
            "key": "EP08_S0051_0029",
            "value": "みんなもこっちに戻ってるかもしれないって\n期待するのは、やっぱり都合が良すぎるかな……？"
        },
        {
            "key": "EP08_S0051_0030",
            "value": "私……ごめんなさい"
        },
        {
            "key": "EP08_S0051_0031",
            "value": "あっ、ごめん！　ミユキのせいだって\n言いたいわけじゃないんだ……！"
        },
        {
            "key": "EP08_S0051_0032",
            "value": "そうだよ！\nミユキは悪くないよー！"
        },
        {
            "key": "EP08_S0051_0033",
            "value": "ち、違うの……！\n私、別のこと考えちゃって……"
        },
        {
            "key": "EP08_S0051_0034",
            "value": "もちろん、他のみんなのことも\n心配なんだけど……っ"
        },
        {
            "key": "EP08_S0051_0035",
            "value": "（あ……そっか\n　きっとミユキはハルのことを心配してるんだ）"
        },
        {
            "key": "EP08_S0051_0036",
            "value": "（当たり前だよな）"
        },
        {
            "key": "EP08_S0051_0037",
            "value": "今って、いつ……なんだろう……？"
        },
        {
            "key": "EP08_S0051_0038",
            "value": "え……？"
        },
        {
            "key": "EP08_S0051_0039",
            "value": "あっ、いえ……私、ずっとぼんやりしてたから\n時間の感覚もわからなくって……"
        },
        {
            "key": "EP08_S0051_0040",
            "value": "ちょっと……気になって……"
        },
        {
            "key": "EP08_S0051_0041",
            "value": "確か、僕たちがあっちの世界に行ってから\n１週間くらいは経ってるはずだけど……"
        },
        {
            "key": "EP08_S0051_0042",
            "value": "（ってことは、こっちでは僕たちが\n　何日も行方不明になってるってことで）"
        },
        {
            "key": "EP08_S0051_0043",
            "value": "（騒ぎになってるはずだよな……？）"
        },
        {
            "key": "EP08_S0051_0044",
            "value": "（これは……マズイかも\n　ミノルたちが戻ってないのに）"
        },
        {
            "key": "EP08_S0051_0045",
            "value": "（今までどこに行ってたとか、みんながまだ\n　あっちの世界にいるってことを説明できない……！）"
        },
        {
            "key": "EP08_S0051_0046",
            "value": "（どうしよう……）"
        },
        {
            "key": "EP08_S0051_0047",
            "value": "あっ、あの……タクマさん！\nちょっと、マズイかも……です！"
        },
        {
            "key": "EP08_S0051_0048",
            "value": "（今度はなんだ？）"
        },
        {
            "key": "EP08_S0051_0049",
            "value": "あっちの人たちが、アグモンのほうを\nちらちら見てる気が……"
        },
        {
            "key": "EP08_S0051_0050",
            "value": "って、そうだった！\nアグモン、急いで隠れて！"
        },
        {
            "key": "EP08_S0051_0051",
            "value": "えっ？　えっ？\nタクマ、どうしてなんだ？？"
        },
        {
            "key": "EP08_S0051_0052",
            "value": "言ったろ？\nこっちの世界じゃアグモンはすごく珍しいんだ"
        },
        {
            "key": "EP08_S0051_0053",
            "value": "もし見つかったら、大変なことになる……！"
        },
        {
            "key": "EP08_S0051_0054",
            "value": "でも～……！"
        },
        {
            "key": "EP08_S0051_0055",
            "value": "少しの間……僕とミユキがあの人たちに\n話を聞くまでの間だけだから、隠れてて……！"
        },
        {
            "key": "EP08_S0051_0056",
            "value": "ん～……わかったよぉ～……"
        },
        {
            "key": "EP08_S0051_0057",
            "value": "少しの間だけだからね～！"
        },
        {
            "key": "EP08_S0051_0058",
            "value": "ふぅ……ひとまず、見つかる前に\n何とかなったかな"
        },
        {
            "key": "EP08_S0051_0059",
            "value": "ふふ……そうだね"
        },
        {
            "key": "EP08_S0051_0060",
            "value": "それで、あの人たちに話を聞くって言ってたけど\n何を聞くの？"
        },
        {
            "key": "EP08_S0051_0061",
            "value": "まずは、こっちの世界で僕たちのことが\nどうなってるかを確認したい"
        },
        {
            "key": "EP08_S0051_0062",
            "value": "僕たちやミユキが、何日も行方不明になってたんだ\n絶対に騒ぎになってると思うんだけど……"
        },
        {
            "key": "EP08_S0051_0063",
            "value": "そ……そうだよね"
        },
        {
            "key": "EP08_S0051_0064",
            "value": "とにかく、<color=#B41919>あの人たち</color>に話を聞いてみよう"
        }
    ]
}