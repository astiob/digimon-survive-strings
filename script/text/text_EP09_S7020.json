{
	"scenario_text":[
		{
			"key":"EP09_S7020_0001",
			"value":"あれ、ラブラモンひとり？\nアオイさんと一緒じゃなかったんだ"
		},
		{
			"key":"EP09_S7020_0002",
			"value":"めずらしいなー！\nいつもアオイと一緒なのに"
		},
		{
			"key":"EP09_S7020_0003",
			"value":"前にも言わなかった？\n私だってひとりで考え事したい時くらいあるの"
		},
		{
			"key":"EP09_S7020_0004",
			"value":"そういえば、そんなことも言ってたっけ"
		},
		{
			"key":"EP09_S7020_0005",
			"value":"それより、タクマに\n聞きたいことがあるんだけど……"
		},
		{
			"key":"EP09_S7020_0006",
			"value":"どうして、こっちに戻ってきたの？"
		},
		{
			"key":"EP09_S7020_0007",
			"value":"ん～？　どういうこと？"
		},
		{
			"key":"EP09_S7020_Q1",
			"value":"（僕が戻ってきた理由は……）"
		},
		{
			"key":"EP09_S7020_Q1_A1",
			"value":"みんなを守りたいから"
		},
		{
			"key":"EP09_S7020_Q1_A2",
			"value":"見て見ぬふりはできないから"
		},
		{
			"key":"EP09_S7020_Q1_A3",
			"value":"力があるからだ"
		},
		{
			"key":"EP09_S7020_0008",
			"value":"みんなを守りたい"
		},
		{
			"key":"EP09_S7020_0009",
			"value":"みんなを守りたいからね\n仲間たちも、この世界も、僕らの世界も"
		},
		{
			"key":"EP09_S7020_0010",
			"value":"……そう、よね"
		},
		{
			"key":"EP09_S7020_0011",
			"value":"見て見ぬふりはできない"
		},
		{
			"key":"EP09_S7020_0012",
			"value":"みんなの前でも言ったけど\n見て見ぬふりはできないからね"
		},
		{
			"key":"EP09_S7020_0013",
			"value":"……そう、よね"
		},
		{
			"key":"EP09_S7020_0014",
			"value":"力があるから"
		},
		{
			"key":"EP09_S7020_0015",
			"value":"僕たちには力があるんだ\nだから、その力で解決できると思ってる"
		},
		{
			"key":"EP09_S7020_0016",
			"value":"……力、かぁ。そうよね"
		},
		{
			"key":"EP09_S7020_0017",
			"value":"えっと……それだけ？"
		},
		{
			"key":"EP09_S7020_0018",
			"value":"……僕、変なこと言ったかな？"
		},
		{
			"key":"EP09_S7020_0019",
			"value":"ううん、なんでもないわ！"
		},
		{
			"key":"EP09_S7020_0020",
			"value":"アオイもみんなも、タクマが戻ってきてくれて\n良かったって心から喜んでると思うわ"
		},
		{
			"key":"EP09_S7020_0021",
			"value":"なら、いいんだけど……"
		},
		{
			"key":"EP09_S7020_0022",
			"value":"さてと……私、そろそろアオイのところに\n戻らなくっちゃ……！"
		},
		{
			"key":"EP09_S7020_0023",
			"value":"変なラブラモン～"
		},
		{
			"key":"EP09_S7020_0024",
			"value":"（……なんだったんだろう）"
		},
		{
			"key":"EP09_S7020_0025",
			"value":"タクマたちが戻ってきてくれて\n本当に良かったって……そう思ってるわ"
		}
	]
}
