	"scenario_text":[
		{
			"key":"EP12_S4111_0001",
			"value":"タクマは帰ったら何がしたい？"
		},
		{
			"key":"EP12_S4111_0002",
			"value":"え？　急だなぁ、なんで？"
		},
		{
			"key":"EP12_S4111_0003",
			"value":"不安な時は、楽しいこと考えた方がいいでしょ"
		},
		{
			"key":"EP12_S4111_0004",
			"value":"ああ、なるほど、そういうことね"
		},
		{
			"key":"EP12_S4111_0005",
			"value":"う～ん、ゆっくりしたい……かな？"
		},
		{
			"key":"EP12_S4111_0006",
			"value":"何それ、おじいちゃんじゃん"
		},
		{
			"key":"EP12_S4111_0007",
			"value":"そういうサキは何がしたいの？"
		},
		{
			"key":"EP12_S4111_0008",
			"value":"ん～、秘密"
		},
		{
			"key":"EP12_S4111_0009",
			"value":"自分から話題振っておいて！？"
		},
		{
			"key":"EP12_S4111_0010",
			"value":"いいじゃん、別に\nあ、でもさ、タクマ"
		},
		{
			"key":"EP12_S4111_0011",
			"value":"どんなに間が空いてもさ\nまたみんなでご飯食べようね"
		},
		{
			"key":"EP12_S4111_0012",
			"value":"うん、そうだね\nそれは約束しよう"
		},
		{
			"key":"EP12_S4111_0013",
			"value":"うん、絶対だよ"
		},
		{
			"key":"EP12_S4111_0014",
			"value":"元の世界に帰ったらさ、また\nみんなでご飯食べようね"
		}
	]
}
