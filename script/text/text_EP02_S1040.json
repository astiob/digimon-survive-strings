{
	"scenario_text":[
		{
			"key":"EP02_S1040_0001",
			"value":"ビックリした！\nハルじゃないか！"
		},
		{
			"key":"EP02_S1040_0002",
			"value":"てっきり、\n何かモンスターでも潜んでるかと……"
		},
		{
			"key":"EP02_S1040_0101",
			"value":"何してるんだ～？"
		},
		{
			"key":"EP02_S1040_0005",
			"value":"うん……パトロールをしてて……"
		},
		{
			"key":"EP02_S1040_Q1",
			"value":"（なるほど、二人きりだからミユキを守ってたのか）"
		},
		{
			"key":"EP02_S1040_Q1_A1",
			"value":"心配させちゃダメだぞ？"
		},
		{
			"key":"EP02_S1040_Q1_A2",
			"value":"感心だな"
		},
		{
			"key":"EP02_S1040_Q1_A3",
			"value":"あれ？　でもさっきまで……"
		},
		{
			"key":"EP02_S1040_0006",
			"value":"熱心なのはいいけど\nお姉さんを心配させちゃダメだぞ？"
		},
		{
			"key":"EP02_S1040_0102",
			"value":"……そんなの、言われなくても当然だよ"
		},
		{
			"key":"EP02_S1040_0103",
			"value":"はは、感心だな\nお姉ちゃんを悪いやつから守ってるんだな"
		},
		{
			"key":"EP02_S1040_0104",
			"value":"……うん"
		},
		{
			"key":"EP02_S1040_0105",
			"value":"さっきまでミユキと一緒にいたのにな"
		},
		{
			"key":"EP02_S1040_0106",
			"value":"…………"
		},
		{
			"key":"EP02_S1040_0107",
			"value":"（はぐれたのかな？）"
		},
		{
			"key":"EP02_S1040_0007",
			"value":"……ハル？"
		},
		{
			"key":"EP02_S1040_0008",
			"value":"あ、お姉ちゃん……"
		},
		{
			"key":"EP02_S1040_0009",
			"value":"……よかった\nここにいたのね"
		},
		{
			"key":"EP02_S1040_0010",
			"value":"うん。……そっか、\nちょっと離れすぎてたね……"
		},
		{
			"key":"EP02_S1040_0011",
			"value":"心配かけて、ゴメンなさい……"
		},
		{
			"key":"EP02_S1040_0012",
			"value":"ううん、いいの……"
		},
		{
			"key":"EP02_S1040_0013",
			"value":"タクマ……さん"
		},
		{
			"key":"EP02_S1040_0014",
			"value":"え？　あ、はい！"
		},
		{
			"key":"EP02_S1040_0015",
			"value":"みつけてくれて……ありがとう……"
		},
		{
			"key":"EP02_S1040_0016",
			"value":"これ……お礼……"
		},
		{
			"key":"EP02_S1040_0017",
			"value":"え？"
		},
		{
			"key":"EP02_S1040_0018",
			"value":"さっき……拾ったの……\n私には使いみちもないし……"
		},
		{
			"key":"EP02_S1040_0019",
			"value":"ええっと……あ、ありがとう"
		},
		{
			"key":"EP02_S1040_0201",
			"value":"……………"
		},
		{
			"key":"EP02_S1040_0202",
			"value":"ミユキがしっかり喋った……？"
		},
		{
			"key":"EP02_S1040_0108",
			"value":"（ハルとミユキは校舎に戻ったみたいだ\n　僕らも探索に戻ろう）"
		}
	]
}
