{
    "scenario_text": [
        {
            "key": "EP04_S0070_0001",
            "value": "いてて……落とされたぞ……"
        },
        {
            "key": "EP04_S0070_0002",
            "value": "ここ、どこだ～？"
        },
        {
            "key": "EP04_S0070_0003",
            "value": "み、みんな……無事よね？\nミユキちゃんと、ハルくんは……？"
        },
        {
            "key": "EP04_S0070_0004",
            "value": "ここにいるよ\nお姉ちゃんも、いっしょだよ"
        },
        {
            "key": "EP04_S0070_0005",
            "value": "いるよ……"
        },
        {
            "key": "EP04_S0070_0006",
            "value": "よかった……無事だったのね"
        },
        {
            "key": "EP04_S0070_0007",
            "value": "他のみんなも\n全員、ちゃんといるみたいよ"
        },
        {
            "key": "EP04_S0070_0008",
            "value": "ていうか、遊園地のお城に\nなんでこんな仕掛けあるんだよ……"
        },
        {
            "key": "EP04_S0070_0009",
            "value": "こんなわけのわからない世界なら\nなんでもありということなのか。あるいは……"
        },
        {
            "key": "EP04_S0070_0010",
            "value": "誰かが何か改造をしてても\nおかしくはないかも知れないな……"
        },
        {
            "key": "EP04_S0070_0011",
            "value": "誰が？　モンスターたちが？"
        },
        {
            "key": "EP04_S0070_0012",
            "value": "ボクたちでも、こんなすごいものを\n作れちゃうって……こと？"
        },
        {
            "key": "EP04_S0070_0013",
            "value": "さあ……？"
        },
        {
            "key": "EP04_S0070_0014",
            "value": "僕に聞かれても、お前たちみたいなやつのことなんて\nわかるわけないだろ"
        },
        {
            "key": "EP04_S0070_0015",
            "value": "そ、そうだよね……"
        },
        {
            "key": "EP04_S0070_0016",
            "value": "コイツらが作ったってより、もっとワケわかんねえ力が\n働いてるっつー感じな気がするけどな……"
        },
        {
            "key": "EP04_S0070_0017",
            "value": "やれやれ……それにしても、\nとんでもねーおてんばだな、カイトの妹は"
        },
        {
            "key": "EP04_S0070_0018",
            "value": "すまん……アイツがああなったのには\n訳があるんだが……"
        },
        {
            "key": "EP04_S0070_0019",
            "value": "どんな訳が？"
        },
        {
            "key": "EP04_S0070_0020",
            "value": "昔ちょっと、辛い目に遭ってな……"
        },
        {
            "key": "EP04_S0070_0021",
            "value": "だからオレは、ミウを何が何でも\n守ってやるって誓ったんだ。なのに……"
        },
        {
            "key": "EP04_S0070_0022",
            "value": "（カイトは、ミウのことを話す時にいつも\n　苦しそうな顔をするけど……）"
        },
        {
            "key": "EP04_S0070_0023",
            "value": "（いったい、何があったんだろう……？）"
        },
        {
            "key": "EP04_S0070_0024",
            "value": "（あんまり詳しく聞かないほうがいいのかな）"
        },
        {
            "key": "EP04_S0070_0025",
            "value": "カイトとミウちゃんが、なんか抱えてるのは\nわかったけど……"
        },
        {
            "key": "EP04_S0070_0026",
            "value": "だからって、過保護すぎるのも\n逆効果だったんじゃないかしら？"
        },
        {
            "key": "EP04_S0070_0027",
            "value": "そうか？"
        },
        {
            "key": "EP04_S0070_0028",
            "value": "オレはただ、心配なだけなんだけど……"
        },
        {
            "key": "EP04_S0070_0029",
            "value": "そういうの……されるほうは\n息苦しかったりするのにね……"
        },
        {
            "key": "EP04_S0070_0030",
            "value": "（あれ……？\n　今、サキがすごく暗い顔してたような……？）"
        },
        {
            "key": "EP04_S0070_0031",
            "value": "サキ？　どうかしたの？"
        },
        {
            "key": "EP04_S0070_0032",
            "value": "ううん、何でもないよー"
        },
        {
            "key": "EP04_S0070_0033",
            "value": "（気のせい……だったかな）"
        },
        {
            "key": "EP04_S0070_0034",
            "value": "とにかくさ、まずはこっからどうやって出るかを\n考えようぜ"
        },
        {
            "key": "EP04_S0070_0035",
            "value": "黙ってここに留まっているわけには\nいかないからな"
        },
        {
            "key": "EP04_S0070_0036",
            "value": "それじゃあ、どこかに<color=#B41919>出口</color>がないか\nみんなで探そうか"
        },
        {
            "key": "EP04_S0070_0037",
            "value": "わかったー！\nまた、みんなで探索だなー！"
        }
    ]
}