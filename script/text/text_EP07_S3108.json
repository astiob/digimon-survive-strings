{
    "scenario_text": [
        {
            "key": "EP07_S3108_0001",
            "value": "タクマくん！\nこれを見てほしいの"
        },
        {
            "key": "EP07_S3108_0002",
            "value": "壁に……傷痕？"
        },
        {
            "key": "EP07_S3108_0003",
            "value": "他の崩れ方に比べて新しい気がして\nもしかしたらピエモンがつけたのかも"
        },
        {
            "key": "EP07_S3108_0004",
            "value": "知らず知らずのうちに引っかけたとかかな"
        },
        {
            "key": "EP07_S3108_0005",
            "value": "それにしても、こんな小さな痕跡を\nよく見つけられたね"
        },
        {
            "key": "EP07_S3108_0006",
            "value": "あ……私、必死で\nなんでもいいから探さなきゃって"
        },
        {
            "key": "EP07_S3108_0007",
            "value": "（アオイさん、褒められて戸惑ってる？\n　照れてるのとも少し違うような）"
        },
        {
            "key": "EP07_S3108_0008",
            "value": "（せっかく手がかりを見つけてくれたんだ\n　この勢いを削ぎたくないけど）"
        },
        {
            "key": "EP07_S3108_0009",
            "value": "（どうしたのか、聞いてみようかな？\n　それともそっとしておくべきか……？）"
        },
        {
            "key": "EP07_S3108_Q1",
            "value": "（アオイさんに何と言おう）"
        },
        {
            "key": "EP07_S3108_Q1_A1",
            "value": "褒められて照れてる？"
        },
        {
            "key": "EP07_S3108_Q1_A2",
            "value": "仕事ができて凄い"
        },
        {
            "key": "EP07_S3108_Q1_A3",
            "value": "この調子でお願い"
        },
        {
            "key": "EP07_S3108_0010",
            "value": "褒められて照れてる？"
        },
        {
            "key": "EP07_S3108_0011",
            "value": "アオイさん……褒められて照れてる？"
        },
        {
            "key": "EP07_S3108_0012",
            "value": "そ、そう見えたかしら？"
        },
        {
            "key": "EP07_S3108_0013",
            "value": "ごめんなさい、こんな時なのに\n褒められて嬉しいとか言っている場合じゃないわよね"
        },
        {
            "key": "EP07_S3108_0014",
            "value": "ええ？　別にいいのに\n成果をあげて凄いじゃないか"
        },
        {
            "key": "EP07_S3108_0015",
            "value": "タクマくん、そういうの今はいいから！\nそうじゃなくて、ええと……"
        },
        {
            "key": "EP07_S3108_0016",
            "value": "仕事ができて凄い"
        },
        {
            "key": "EP07_S3108_0017",
            "value": "きちんと成果をあげて、アオイさんは偉いね\n仕事ができるってかっこいいと思うよ"
        },
        {
            "key": "EP07_S3108_0018",
            "value": "そんなんじゃない！！"
        },
        {
            "key": "EP07_S3108_0019",
            "value": "えっ？　アオイさん？"
        },
        {
            "key": "EP07_S3108_0020",
            "value": "私がもっとしっかりしてれば、こんな状況に\nそもそも陥らなかったかもしれないわ"
        },
        {
            "key": "EP07_S3108_0021",
            "value": "そんな風に思いつめなくても……"
        },
        {
            "key": "EP07_S3108_0022",
            "value": "あ……ご、ごめんなさい\nそれこそ今言うことじゃないわよね……"
        },
        {
            "key": "EP07_S3108_0023",
            "value": "この調子でお願い"
        },
        {
            "key": "EP07_S3108_0024",
            "value": "ありがとう、アオイさん\nこの調子でお願いするよ"
        },
        {
            "key": "EP07_S3108_0025",
            "value": "こうやって少しずつでも何か見つけられれば\nきっとミユキもピエモンも見つけられるよね"
        },
        {
            "key": "EP07_S3108_0026",
            "value": "ええ、そうね！\n役に立てたならよかった……！"
        },
        {
            "key": "EP07_S3108_0027",
            "value": "……あっ、ご、ごめんなさい\n喜んでる場合じゃないわよね"
        },
        {
            "key": "EP07_S3108_0028",
            "value": "と、とにかく……この傷痕\n北に向かっているように見えない？"
        },
        {
            "key": "EP07_S3108_0029",
            "value": "北か……たしかに建物が見えるけど"
        },
        {
            "key": "EP07_S3108_0030",
            "value": "でもあそこに繋がる道なんて\n今のところ見当たらないよね"
        },
        {
            "key": "EP07_S3108_0031",
            "value": "今度はそれを探しましょう\n急ぐから、待ってて！"
        },
        {
            "key": "EP07_S3108_0032",
            "value": "あ、アオイさん……"
        },
        {
            "key": "EP07_S3108_0033",
            "value": "今は張り切ったままにさせてあげて\nきっと、怖くて立ち止まれないのよ"
        },
        {
            "key": "EP07_S3108_0034",
            "value": "そっか、わかったよ"
        },
        {
            "key": "EP07_S3108_0035",
            "value": "（怖さが原動力になってる内はいいけど……\n　いつか爆発しないかな、心配だ）"
        },
        {
            "key": "EP07_S3108_0036",
            "value": "年上なのに引っ張ってもらってごめんね\n役に立てるよう、頑張るから"
        },
        {
            "key": "EP07_S3108_0037",
            "value": "（そういえば、そろそろ教授の具合も\n　よくなったかな？）"
        },
        {
            "key": "EP07_S3108_0038",
            "value": "（<color=#B41919>様子</color>を見に行ってみよう）"
        }
    ]
}