	"scenario_text":[
		{
			"key":"EP02_S2000_0001",
			"value":"ふぅ……これで探せるところは全部見たな"
		},
		{
			"key":"EP02_S2000_0002",
			"value":"ショクリョウはあったのか？"
		},
		{
			"key":"EP02_S2000_0003",
			"value":"うーん、ミノルのおかげで少しは見つかったけど\n正直なところ、もう少し欲しいんだよな"
		},
		{
			"key":"EP02_S2000_0004",
			"value":"でも、もう他に探せるところなんて……"
		},
		{
			"key":"EP02_S2000_0005",
			"value":"みんなーー！　こっち来て！\n体育館！"
		},
		{
			"key":"EP02_S2000_0006",
			"value":"うわっ！　なんだ！？　アオイさん！？"
		},
		{
			"key":"EP02_S2000_0007",
			"value":"この中から聞こえたよ！"
		},
		{
			"key":"EP02_S2000_0008",
			"value":"何かあったのかもしれない！"
		},
		{
			"key":"EP02_S2000_0009",
			"value":"すぐ行こう！"
		},
		{
			"key":"EP02_S2000_0010",
			"value":"いや、アオイさんの声が届いてない人もいるはずだ\nみんなにも声をかけて行ってみよう！"
		},
		{
			"key":"EP02_S2000_0011",
			"value":"わかった！"
		}
	]
