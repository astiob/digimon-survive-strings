{
    "scenario_text": [
        {
            "key": "EP07_S3010_0001",
            "value": "ようやくついたか\nそれにしても静かだな……"
        },
        {
            "key": "EP07_S3010_0002",
            "value": "ここから眺めてるだけじゃ、\nどこが入り口かわかんねーな"
        },
        {
            "key": "EP07_S3010_0003",
            "value": "あいつらが出入りしてるんだ\n人が通れる隙間くらいあるだろ"
        },
        {
            "key": "EP07_S3010_0004",
            "value": "うん、調べてみよう！"
        },
        {
            "key": "EP07_S3010_0005",
            "value": "キュキュゥッ！"
        },
        {
            "key": "EP07_S3010_0006",
            "value": "おい！　クネモンが入り口見つけたってよ！"
        },
        {
            "key": "EP07_S3010_0007",
            "value": "本当だ\nありがとうクネモン！"
        },
        {
            "key": "EP07_S3010_0008",
            "value": "あっ！　見てタクマ、入り口がある！\nここから工場の中に入れるみたいだぞ！"
        },
        {
            "key": "EP07_S3010_0009",
            "value": "本当だ\n偉いぞアグモン、よく見つけたね！"
        },
        {
            "key": "EP07_S3010_0010",
            "value": "中は……ずいぶん広そうね"
        },
        {
            "key": "EP07_S3010_0011",
            "value": "ちょっと危ないかもしれないけど\n手分けして探すべきかな？"
        },
        {
            "key": "EP07_S3010_0012",
            "value": "だろうな、ここは敵の本拠地だ\n仮に罠があったとして……"
        },
        {
            "key": "EP07_S3010_0013",
            "value": "全員一緒に引っかかったら\n助けられなくなる"
        },
        {
            "key": "EP07_S3010_0014",
            "value": "そうだね\n手分けして探そう"
        },
        {
            "key": "EP07_S3010_0015",
            "value": "わかったけど……\nでも絶対に無理はしないでね"
        },
        {
            "key": "EP07_S3010_0016",
            "value": "ミユキちゃんやピエモンを見かけても\n焦って突撃しちゃダメよ"
        },
        {
            "key": "EP07_S3010_0017",
            "value": "うん\n慎重に<color=#B41919>辺り</color>を探って行こう"
        }
    ]
}