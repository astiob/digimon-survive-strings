{
    "scenario_text": [
        {
            "key": "EP13_S9101_0001",
            "value": "とうとう、来たな……"
        },
        {
            "key": "EP13_S9101_0002",
            "value": "元の世界でここに来たのが\n始まりだったんだよな……"
        },
        {
            "key": "EP13_S9101_0003",
            "value": "う、ウチが連れてきたせいで……\nごめんね……"
        },
        {
            "key": "EP13_S9101_0004",
            "value": "あっ、いやいや！\n全然ミウのせいなんかじゃないって！"
        },
        {
            "key": "EP13_S9101_0005",
            "value": "おい！　なにミウを悲しませてんだコラぁ！"
        },
        {
            "key": "EP13_S9101_0006",
            "value": "コラっ！　こんな時に喧嘩しないの！"
        },
        {
            "key": "EP13_S9101_0007",
            "value": "大丈夫よ。誰もミウちゃんのせいだなんて\n思ってないから"
        },
        {
            "key": "EP13_S9101_0008",
            "value": "っつーか、お前に案内させたのオレだしな……"
        },
        {
            "key": "EP13_S9101_0009",
            "value": "ほらほら、誰が悪いとかじゃないっしょ？\nフコーな事故！　そんだけ！"
        },
        {
            "key": "EP13_S9101_0010",
            "value": "う、うん……みんな、ありがと……"
        },
        {
            "key": "EP13_S9101_0011",
            "value": "さて、目的の祠はこの裏手だが……"
        },
        {
            "key": "EP13_S9101_0012",
            "value": "ようやくですね……\n緊張でここまでの道程が果てしなく長く感じました"
        },
        {
            "key": "EP13_S9101_0013",
            "value": "んっ！？　みんな気をつけて！\n嫌な気配がする！"
        },
        {
            "key": "EP13_S9101_0014",
            "value": "て、敵か！？"
        },
        {
            "key": "EP13_S9101_0015",
            "value": "タブン……\nまだ遠くだから大丈夫だと思うけど"
        },
        {
            "key": "EP13_S9101_0016",
            "value": "クンクン……これ、<color=#B41919>祠の方</color>だわ"
        },
        {
            "key": "EP13_S9101_0017",
            "value": "そんな……！　近くなったとは言え\nかなり距離はあるぞ！？"
        },
        {
            "key": "EP13_S9101_0018",
            "value": "キュキュー……！"
        },
        {
            "key": "EP13_S9101_0019",
            "value": "間違いねえ、祠からハンパねえ気配が溢れてる\nそう、クネモンが断言してる"
        },
        {
            "key": "EP13_S9101_0020",
            "value": "これ、やっぱ主だよな……？"
        },
        {
            "key": "EP13_S9101_0021",
            "value": "こんな距離で気配を感じるなんて\nすごくマズイよ！"
        },
        {
            "key": "EP13_S9101_0022",
            "value": "タクマ、急いだ方がいいぞ！"
        },
        {
            "key": "EP13_S9101_0023",
            "value": "よし、みんな行こう！"
        }
    ]
}