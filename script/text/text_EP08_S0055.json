	"scenario_text":[
		{
			"key":"EP08_S0055_0001",
			"value":"この景色……"
		},
		{
			"key":"EP08_S0055_0002",
			"value":"（ミユキ、どうしたんだろう？）\nこっちの世界に戻ってからソワソワしてるような……）"
		},
		{
			"key":"EP08_S0055_0003",
			"value":"何か、探してるの？"
		},
		{
			"key":"EP08_S0055_0004",
			"value":"へっ！？"
		},
		{
			"key":"EP08_S0055_0005",
			"value":"さっきからずっとキョロキョロしてるから……"
		},
		{
			"key":"EP08_S0055_0006",
			"value":"あ……うん\nちょっと景色が……その……"
		},
		{
			"key":"EP08_S0055_0007",
			"value":"景色？\n景色が、どうかしたの？"
		},
		{
			"key":"EP08_S0055_0008",
			"value":"っ……や、やっぱり何でもない……！"
		},
		{
			"key":"EP08_S0055_0009",
			"value":"何か気になることがあるなら\n遠慮なく言っていいからな？"
		},
		{
			"key":"EP08_S0055_0010",
			"value":"何でも聞くよ？"
		},
		{
			"key":"EP08_S0055_0011",
			"value":"ありがとう……タクマさん"
		},
		{
			"key":"EP08_S0055_0012",
			"value":"でも、もうちょっと自分で確かめてみて……\nそれからタクマさんにも話すね……！"
		},
		{
			"key":"EP08_S0055_0013",
			"value":"う、うん"
		},
		{
			"key":"EP08_S0055_0014",
			"value":"（よくわからないけど、ミユキが話したくなるまで\n　そっとしておくか……）"
		},
		{
			"key":"EP08_S0055_0015",
			"value":"気になることがあるけど……もうちょっとハッキリしたら\nタクマさんにも話すね……！"
		}
	]
}
