{
	"scenario_text":[
		{
			"key":"EP99_S7300_0001",
			"value":"あ、タクマ\nちょうどいいタイミング！"
		},
		{
			"key":"EP99_S7300_0002",
			"value":"ん？　僕に用事でもあったの？"
		},
		{
			"key":"EP99_S7300_0003",
			"value":"まあね……へへへ\n実はさ、ちょっと相談があるんだけど……"
		},
		{
			"key":"EP99_S7300_0004",
			"value":"アオイ……の事なんだけど"
		},
		{
			"key":"EP99_S7300_0005",
			"value":"アオイ？　彼女がどうしたの？"
		},
		{
			"key":"EP99_S7300_0006",
			"value":"アオイってさ……\nなんか苦手なんだよね"
		},
		{
			"key":"EP99_S7300_0007",
			"value":"ほら、アオイってさ\nいつも言いたい事をガマンしている感じでしょ？"
		},
		{
			"key":"EP99_S7300_0008",
			"value":"だったら言えばいいのにって思うと\nちょっとストレスが溜まるっていうか……"
		},
		{
			"key":"EP99_S7300_0009",
			"value":"だったらそう言えばいいのに"
		},
		{
			"key":"EP99_S7300_0010",
			"value":"いやそうなんだけど\nでも、言えばカドが立たない？"
		},
		{
			"key":"EP99_S7300_0011",
			"value":"う～ん、それはそうだけど……"
		},
		{
			"key":"EP99_S7300_0012",
			"value":"だからこうしてタクマに相談してるんだけど\nねえ、どうしたらいいと思う？"
		},
		{
			"key":"EP99_S7300_0013",
			"value":"（う～ん……\nどう返事をするのがベストなのか……）"
		},
		{
			"key":"EP99_S7300_0014",
			"value":"（サキが苦手だと思うなら\n距離を置けばいい）"
		},
		{
			"key":"EP99_S7300_0015",
			"value":"（でもわざわざ僕に相談するということは……）"
		},
		{
			"key":"EP99_S7300_Q1",
			"value":"サキへの返答は……"
		},
		{
			"key":"EP99_S7300_Q1_A1",
			"value":"アオイと仲良くしたいの？"
		},
		{
			"key":"EP99_S7300_Q1_A2",
			"value":"まさか果たし合いを！？"
		},
		{
			"key":"EP99_S7300_Q1_A3",
			"value":"もしかして僕が好きなの？"
		},
		{
			"key":"EP99_S7300_0016",
			"value":"サキはアオイと仲良くしたいの？"
		},
		{
			"key":"EP99_S7300_0017",
			"value":"……正直、わかんない"
		},
		{
			"key":"EP99_S7300_0018",
			"value":"確かにアオイは苦手\nでも嫌いじゃない……かも"
		},
		{
			"key":"EP99_S7300_0019",
			"value":"だからこうして\n相談しているんだけど……"
		},
		{
			"key":"EP99_S7300_0020",
			"value":"（うーん、迷っている様子だな\nでも嫌いじゃない……とも言っている）"
		},
		{
			"key":"EP99_S7300_0021",
			"value":"……アオイは私の事を\nどう思っているのかな……"
		},
		{
			"key":"EP99_S7300_0022",
			"value":"まさか果し合いを考えているの？"
		},
		{
			"key":"EP99_S7300_0023",
			"value":"ほら、白黒はっきり付けたいとか"
		},
		{
			"key":"EP99_S7300_0024",
			"value":"……タクマ\n私がマジでそんな事を考えていると思う？"
		},
		{
			"key":"EP99_S7300_0025",
			"value":"いや、別にそういうわけじゃ……"
		},
		{
			"key":"EP99_S7300_0026",
			"value":"頭……マジで大丈夫？"
		},
		{
			"key":"EP99_S7300_0027",
			"value":"（うーん、どうやら\n激しく嫌っているわけではなさそうだ）"
		},
		{
			"key":"EP99_S7300_0028",
			"value":"もしかしてさ\nサキは僕のことが好きなの？"
		},
		{
			"key":"EP99_S7300_0029",
			"value":"え？　どうしてそういう話になるの？\n意味わかんない"
		},
		{
			"key":"EP99_S7300_0030",
			"value":"あ、いや！　そうじゃなくって！\nだから……"
		},
		{
			"key":"EP99_S7300_0031",
			"value":"だってわざわざ僕に相談するって\n事は僕が気になるって事で……"
		},
		{
			"key":"EP99_S7300_0032",
			"value":"タクマ、話がぶっ飛びすぎ"
		},
		{
			"key":"EP99_S7300_0033",
			"value":"え、ちがうの！？"
		},
		{
			"key":"EP99_S7300_0034",
			"value":"全然違うよ！！"
		},
		{
			"key":"EP99_S7300_0035",
			"value":"はあ、とにかく……"
		},
		{
			"key":"EP99_S7300_0036",
			"value":"なんかいい方法ないかな……"
		}
	]
}
