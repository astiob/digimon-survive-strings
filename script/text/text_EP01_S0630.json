{
    "scenario_text": [
        {
            "key": "EP01_S0630_0100",
            "value": "（え？　雪が、止んでる……？）"
        },
        {
            "key": "EP01_S0630_0034",
            "value": "あれ？　吹雪は？"
        },
        {
            "key": "EP01_S0630_0035",
            "value": "そんなに長いトンネルじゃないのに……\nどういうことかしら"
        },
        {
            "key": "EP01_S0630_0024",
            "value": "はは、ラッキー！\nすっかり止んでるじゃん！"
        },
        {
            "key": "EP01_S0630_0025",
            "value": "と言うより、トンネルのこちら側は全然\n降ってなかったって様子よね"
        },
        {
            "key": "EP01_S0630_0036",
            "value": "それにこの花……なんかすごくない？\n森一面真っ赤だ"
        },
        {
            "key": "EP01_S0630_0101",
            "value": "何だっけ……図鑑で見たことある気が……"
        },
        {
            "key": "EP01_S0630_0102",
            "value": "これって……彼岸花よね、確か"
        },
        {
            "key": "EP01_S0630_0103",
            "value": "そうだそうだ。それだ"
        },
        {
            "key": "EP01_S0630_0104",
            "value": "でも、彼岸花って確か……春じゃなくて"
        },
        {
            "key": "EP01_S0630_0105",
            "value": "そう、秋に咲く花"
        },
        {
            "key": "EP01_S0630_0106",
            "value": "一体……どういうこと？"
        },
        {
            "key": "EP01_S0630_0107",
            "value": "（ん？　アオイさんが何かを気にしている……？）"
        },
        {
            "key": "EP01_S0630_0037",
            "value": "（神社に向かう前に\n　少し<color=#B41919>この辺り</color>を調べてみようか）"
        }
    ]
}