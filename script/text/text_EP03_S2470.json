	"scenario_text":[
		{
			"key":"EP03_S2470_0001",
			"value":"よう、また会ったな！"
		},
		{
			"key":"EP03_S2470_0002",
			"value":"やっほ～"
		},
		{
			"key":"EP03_S2470_0003",
			"value":"あん？　なんだ、お前もこっち来たのかよ、タクマ"
		},
		{
			"key":"EP03_S2470_0004",
			"value":"なんだじゃないよ、まったく勝手に一人で行動して"
		},
		{
			"key":"EP03_S2470_0005",
			"value":"お前だって、勝手にオレについてきてんじゃねえか"
		},
		{
			"key":"EP03_S2470_0006",
			"value":"いや、せっかく再会できた仲間を\n一人にはしたくないだろ……"
		},
		{
			"key":"EP03_S2470_0007",
			"value":"ふん、そうかよ\n小せえオマケを連れてくるくらい心配だったか？"
		},
		{
			"key":"EP03_S2470_0008",
			"value":"オマケ……？　あぁ、二人とは初めてだよね"
		},
		{
			"key":"EP03_S2470_0009",
			"value":"一緒に行動してるミユキとハルだよ"
		},
		{
			"key":"EP03_S2470_0010",
			"value":"よろしく……"
		},
		{
			"key":"EP03_S2470_0011",
			"value":"よろしく……"
		},
		{
			"key":"EP03_S2470_0012",
			"value":"ま、なんでもいいけどよ"
		},
		{
			"key":"EP03_S2470_0013",
			"value":"勝手についてきたんだから\n足手まといになるんじゃねえぞ"
		}
	]
}
