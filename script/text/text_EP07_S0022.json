	"scenario_text":[
		{
			"key":"EP07_S0022_0001",
			"value":"みんな、おまたせ！"
		},
		{
			"key":"EP07_S0022_0002",
			"value":"あー！　やーっと来たか！\n遅いぞタクマ！"
		},
		{
			"key":"EP07_S0022_0003",
			"value":"教授も、何やってんですか！"
		},
		{
			"key":"EP07_S0022_0004",
			"value":"すまなかった、つい祠に夢中になってな"
		},
		{
			"key":"EP07_S0022_0005",
			"value":"まったく、先が思いやられるぜ"
		},
		{
			"key":"EP07_S0022_0006",
			"value":"ごめんごめん。でも、これで全員揃ったな"
		},
		{
			"key":"EP07_S0022_0007",
			"value":"じゃあ……行くんだよね\nこの水路の中に"
		},
		{
			"key":"EP07_S0022_0008",
			"value":"昨日の今日でここに入るのはちっと怖いけど\n泣き言なんか言ってらんねーよな"
		},
		{
			"key":"EP07_S0022_0009",
			"value":"……行きますか！\n敵を派手にぶっ倒しにさ！"
		},
		{
			"key":"EP07_S0022_0010",
			"value":"うん、行こう\n僕たちは進まないといけないんだから"
		}
	]
}
